  <div class="text-center">
  <h1>Create a Market</h1>
  <%= form_for @farmer, :multipart => true do |f| %>
  <div>
  <%= f.label :farmer_market_name%>
  <%= f.text_field :farmer_market_name, class:"form-control"%>
  </div>

  <div >
  <%=f.label :schedule%>
  <%=f.text_field :schedule, class:"form-control"%>
  </div>

  <div>
  <%=f.file_field :avatar, class:"form-control" %> 
  </div>

  <div>
  <%=f.label :speciality%>
  <%=f.text_area :speciality, class:"form-control"%>
  </div>

  <%=f.label :price_range%>
  <%=f.text_field :price_range, class:"form-control"%>
  <%=f.submit "Create"%>
  <%end%>

</div>